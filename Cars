#include <iostream>
#include <string>

class Vehicle {
public:
    std::string carName;
    std::string id;

    Vehicle(std::string name, std::string vehicleId) : carName(name), id(vehicleId) {}
};

class Car : public Vehicle {
public:
    Car(std::string name, std::string vehicleId) : Vehicle(name, vehicleId) {}
};

int main() {
    Car myCar("Toyota Camry", "C-2025-101122");

    std::cout << "Car Name: " << myCar.carName << std::endl;
    std::cout << "Vehicle ID: " << myCar.id << std::endl;

    return 0;
}
